Option Explicit

Public Event Hide()
Dim rslg1 As ADODB.Recordset



Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
   If UnloadMode = vbFormControlMenu Then
      ' Notify parent that user clicked [X].
      Cancel = True
      RaiseEvent Hide
   End If
   Set rslg1 = New ADODB.Recordset
   
   rslg1.Open ("SELECT Lgota.NomNum, Lgota.Numer, Lgota.NAME_KLS, Lgota.OhteCode From Lgota WHERE (((Lgota.NomNum)=" + Potok.Num + "))")
   
   Set fg1.DataSource = rslg1
   
End Sub
